<h1>Busca</h1>

<div class="card z-depth-4"></div>
  <div class="card-content">
    <br/>
    <div id="map-size"></div>
  </div>

<style>
  #map-size {
    width: 100%;
    height: 400px;
  }
</style>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAEFL4I3GmmiTxOjSvAKZo2r_E1Pk6weDU&callback=initMap" 
async defer></script>


<body>
  <div id="map">
  <script>
    function initMap() {
      var mapCanvas = document.getElementById('map-size');
      var mapOptions = {
        center: new google.maps.LatLng(-8.0538900, -34.8811100),
        zoom: 10,
        mapTypeId: google.maps.MapTypeId.ROADMAP
      }
      var map = new google.maps.Map(mapCanvas, mapOptions);
      var marker = new google.maps.Marker({
        title: "Recife",
        position: new google.maps.LatLng(-8.0538900, -34.8811100)
      });
      marker.setMap(map);
    }
    google.maps.event.addDomListener(window, 'load', initialize);
  </script>
  </div>
</body>