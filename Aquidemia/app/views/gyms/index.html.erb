<p id="notice"><%= notice %></p>

<h1>Gyms</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Rpa</th>
      <th>Microregion</th>
      <th>Cnes</th>
      <th>Name</th>
      <th>Adress</th>
      <th>Bairro</th>
      <th>Phone</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Hour</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gyms.each do |gym| %>
      <tr>
        <td><%= gym.number %></td>
        <td><%= gym.rpa %></td>
        <td><%= gym.microregion %></td>
        <td><%= gym.cnes %></td>
        <td><%= gym.name %></td>
        <td><%= gym.adress %></td>
        <td><%= gym.bairro %></td>
        <td><%= gym.phone %></td>
        <td><%= gym.latitude %></td>
        <td><%= gym.longitude %></td>
        <td><%= gym.hour %></td>
        <td><%= gym.rating %></td>
        <td><%= link_to 'Show', gym %></td>
        <td><%= link_to 'Edit', edit_gym_path(gym) %></td>
        <td><%= link_to 'Destroy', gym, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gym', new_gym_path %>
